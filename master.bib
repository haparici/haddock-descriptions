@article{larson:1991,
	Annote = {Larson (1991) attributes the curious control behavior of the verb {\em promise} to its occurring syntactically in the ditransitive construction, as in {\em I promised (you) a rose garden}.  He compares promise to numerous other verbs that do not occur in the ditransitive construction and do not take subject control.  However, there are three important omissions in his account.  (1) Larson does not look at all at the behavior of the nominal {\em promise}, in particular at the facts adduced here, which in fact have been in the literature since at least Jackendoff 1974; (2) he does not look at the verb {\em pledge}, which, as shown in 38b [John vowed to/pledged to/agreed with/is obligated to Susan to take care of himself/herself/oneself.] does not occur in a ditransitive but still allows subject control; and (3) he does not consider the verb {\em tell}, which has almost the same syntactic distribution as {\em promise}, in particular occuring in a ditransitive with optional indirect object ({\em tell (Bill) a story}), yet has object control (section 4.2).  (J&C 2003 footnote 11, p. 531)},
	Author = {Larson, Richard},
	Date-Modified = {2008-12-03 21:22:41 -0600},
	Journal = {Linguistic Inquiry},
	Pages = {103--39},
	Title = {{\em Promise} and the Theory of Control},
